    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Application",
            "request": "launch",
            "mainClass": "genius.Application",
            "projectName": "main"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-CommandLineRunner<main>",
            "request": "launch",
            "mainClass": "genius.core.misc.CommandLineRunner",
            "projectName": "main"
        },
        {
            "type": "java",
            "name": "Run Party",
            "request": "launch",
            "mainClass": "misc.RunSession",
            "projectName": "main",
            "args": "lib\\party.xml logs\\last_run.csv"
        },
        {
            "type": "java",
            "name": "Run MCTS",
            "request": "launch",
            "mainClass": "misc.RunSession",
            "projectName": "main",
            "console": "integratedTerminal",
            "args": "lib\\mcts.xml logs\\last_run.csv"
        },
        {
            "type": "java",
            "name": "Run BinarySearch",
            "request": "launch",
            "mainClass": "misc.RunSession",
            "projectName": "main",
            "args": "lib\\binarysearch.xml logs\\last_run.csv"
        },
        {
            "type": "java",
            "name": "Run Test",
            "request": "launch",
            "mainClass": "misc.RunSession",
            "projectName": "main",
            "args": "lib\\test_certain.xml logs\\last_run.csv"
        },
        {
            "type": "java",
            "name": "Run Test (uncertain)",
            "request": "launch",
            "mainClass": "misc.RunSession",
            "projectName": "main",
            "args": "lib\\test_uncertain.xml logs\\last_run.csv"
        },
        {
            "name": "Run Laptop",
            "request": "launch",
            "mainClass": "misc.RunSession",
            "projectName": "main",
            "args": "lib\\laptop.xml logs\\last_run.csv"
        },
        {
            "type": "java",
            "name": "TOURNAMENT",
            "request": "launch",
            "mainClass": "misc.RunTournament",
            "projectName": "main",
            "args": "lib\\tournament.xml logs\\loggedTournamentStatistics"
        }
    ]
}